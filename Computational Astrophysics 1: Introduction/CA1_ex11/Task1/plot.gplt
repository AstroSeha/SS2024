set terminal png
set output 'task1.png'
set logscale xy
bw = 1e-40
N = 10000
set boxwidth bw
bin(x,s)=s*ceil(x/s)
set style fill solid 0.5
set xlabel 'Path Length'
set ylabel 'Number'

plot 'results.txt' using (bin($1, bw)):(1) smooth frequency with boxes title 'Path Length Distribution' lc rgb "#317ED7"